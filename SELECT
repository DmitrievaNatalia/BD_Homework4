SELECT album_name, year 
FROM albums 
WHERE year = 2018;

SELECT track_name,length
FROM tracks
ORDER BY length DESC
LIMIT 1;

SELECT track_name
FROM tracks 
WHERE length >= 3.5;

SELECT collection_name
FROM collections 
WHERE year BETWEEN 2018 AND 2020;

SELECT singer_name
FROM singers
WHERE singer_name NOT LIKE '% %'; 

SELECT track_name
FROM tracks 
WHERE track_name ILIKE '%my%';